{
  "name": "paith-notes/worker",
  "type": "project",
  "require": {
    "aws/aws-sdk-php": "^3"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1.12",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --standard=/app/phpcs.xml",
    "phpcbf": "vendor/bin/phpcbf --standard=/app/phpcs.xml",
    "phpstan": "vendor/bin/phpstan analyse -c /app/phpstan.neon"
  },
  "autoload": {
    "psr-4": {
      "Paith\\Notes\\Worker\\": "src/",
      "Paith\\Notes\\Shared\\": "../backend-shared/src/"
    }
  }
}
